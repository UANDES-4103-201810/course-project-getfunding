<div class=col-6>




  <p id="notice"><%= notice %></p>

  <h1>User</h1>

  <table class = "table table-hover -bordered">
    <thead>
    <tr>

      <th>Name</th>
      <th>Admin</th>
      <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.admin %></td>

    <% end %>
    </tbody>
  </table>

  <br>
</div>